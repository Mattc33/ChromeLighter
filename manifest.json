{
  "manifest_version": 2,

  "name": "ChromeLighter",
  "description": "A simple highlighting tool for chrome.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "src/images/icon128.png",
    "default_popup": "src/popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
